<Window 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ajan"
        xmlns:Properties="clr-namespace:Ajan.Properties" x:Name="main_windows" x:Class="Ajan.MainWindow"
        mc:Ignorable="d"
        Title="Ajan Control Panel" Height="834" Width="1308.5" WindowStyle="None">
    <Grid Margin="0" RenderTransformOrigin="0.497,0.5" ScrollViewer.VerticalScrollBarVisibility="Disabled" Background="#FF1B1B1B" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="232*"/>
            <ColumnDefinition Width="103*"/>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="661*"/>
            <ColumnDefinition Width="Auto" MinWidth="56"/>
            <ColumnDefinition Width="159*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Grid.LayoutTransform>
            <TransformGroup>
                <ScaleTransform ScaleY="1" ScaleX="1"/>
                <SkewTransform AngleY="0" AngleX="0"/>
                <RotateTransform Angle="0"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.LayoutTransform>
        <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="6" HorizontalAlignment="Left" Height="61" Margin="67,401,0,0" VerticalAlignment="Top" Width="1169" Background="#FF525252" CornerRadius="10"/>

        <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="6" HorizontalAlignment="Left" Height="61" Margin="67,323,0,0" VerticalAlignment="Top" Width="1169" Background="#FF525252" CornerRadius="10"/>
        <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="6" HorizontalAlignment="Left" Height="61" Margin="67,243,0,0" VerticalAlignment="Top" Width="1169" Background="#FF525252" CornerRadius="10"/>

        <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="6" HorizontalAlignment="Left" Height="61" Margin="67,164,0,0" VerticalAlignment="Top" Width="1169" Background="#FF525252" CornerRadius="10"/>
        <Border BorderBrush="Black" BorderThickness="1" Grid.ColumnSpan="6" HorizontalAlignment="Left" Height="61" Margin="67,85,0,0" VerticalAlignment="Top" Width="1169" Background="#FF525252" CornerRadius="10">
            <Rectangle HorizontalAlignment="Left" Height="396" Margin="893,-1,0,-336" Stroke="#FF1B1B1B" VerticalAlignment="Top" Width="5" Fill="#FF1B1B1B"/>
        </Border>

        <Button x:Name="StartAjan_btn" Content="   Start AJAN                 " Click="startAjan" HorizontalAlignment="Left" Margin="10,580,0,0" VerticalAlignment="Top" Width="122" Height="41" FontSize="18" Background="#FF28A745" Foreground="#FFF8FAFF" Grid.Column="3" FontFamily="L"/>
        <Button x:Name="StartTriplestore_btn" Content="     Start Triple Store                 " Click="startTripleStore" HorizontalAlignment="Left" Margin="56,664,0,0" VerticalAlignment="Top" Width="202" Height="41" FontSize="18" Background="#FF28A745" Foreground="#FFF8FAFF" Grid.Column="1" Grid.ColumnSpan="3"/>
        <Button x:Name="StartEditor_btn" Click="startEditor" HorizontalAlignment="Left" Margin="102,664,0,0" VerticalAlignment="Top" Width="114" Height="41" FontSize="18" Background="#FF28A745" Foreground="#FFF8FAFF" Grid.Column="3" Content="Start Editor"/>
        <Button x:Name="StartExecutionservice_btn" Content="  Start Execution Service                 " Click="startExectionService" HorizontalAlignment="Left" Margin="259,664,0,0" VerticalAlignment="Top" Width="202" Height="41" FontSize="18" Background="#FF28A745" Foreground="#FFF8FAFF" RenderTransformOrigin="-1.748,0.584" Grid.Column="3"/>
        <Button x:Name="ExitAjan_btn" Content="    Exit AJAN          " Click="ExitEditor" Margin="195,580,344,0" VerticalAlignment="Top" Height="41" FontSize="18" Background="#FFCC4B37" Foreground="#FFF8FAFF" Grid.Column="3"/>
        <Label x:Name="java_version_label" Content="" HorizontalAlignment="Left" Margin="159,98,0,0" VerticalAlignment="Top" Width="533" Height="40" Foreground="#FFF8FAFF" FontSize="18" FontFamily="Sylfaen" Grid.ColumnSpan="4"/>
        <Button x:Name="install_java_btn" Content="Install OpenJDK 1.8" Click="installJava" HorizontalAlignment="Left" Margin="564,97,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Grid.Column="3" Grid.ColumnSpan="3" />
        <Label x:Name="bower_version_label" Content="" HorizontalAlignment="Left" Margin="159,335,0,0" VerticalAlignment="Top" Width="533" Height="37" Foreground="#FFF8FAFF" FontSize="18" TextOptions.TextHintingMode="Animated" UseLayoutRounding="False" FontFamily="Sylfaen" Grid.ColumnSpan="4"/>
        <Grid Grid.ColumnSpan="4" Margin="229,178,14,609">
            <Label x:Name="maven_version_label" Content="" HorizontalAlignment="Left" Margin="-73,0,0,0" VerticalAlignment="Top" Width="533" Height="37" Foreground="#FFF8FAFF" FontSize="18" TextOptions.TextHintingMode="Animated" UseLayoutRounding="False" FontFamily="Sylfaen"/>
            <Button x:Name="install_Maven_btn" Content="Install Maven 3.9.9" Click="installMaven" HorizontalAlignment="Left" Margin="759,0,-117,0" Width="200" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Height="35" VerticalAlignment="Top"/>
            <Button x:Name="path_maven_btn" Content="path to Maven 3.9.9" Click="pathToMaven" HorizontalAlignment="Left" Margin="511,2,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D"  />
            <Image x:Name="maven_install_sign" HorizontalAlignment="Left" Margin="-125,-1,0,0" Width="30" Source="greenTick.png" RenderTransformOrigin="0.486,0.529" Height="59" VerticalAlignment="Top"/>
        </Grid>
        <Grid Grid.ColumnSpan="4" Margin="230,257,14,532">
            <Label x:Name="node_version_label" Content="" HorizontalAlignment="Left" Margin="-73,0,0,0" VerticalAlignment="Top" Width="533" Height="37" Foreground="#FFF8FAFF" FontSize="18" TextOptions.TextHintingMode="Animated" UseLayoutRounding="False" FontFamily="Sylfaen"/>
            <Button x:Name="path_nodejs_btn" Content="path to NodeJS 8.6" Click="pathToNodejs" HorizontalAlignment="Left" Margin="510,3,0,-1" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D"  />
            <Image x:Name="node_install_sign" HorizontalAlignment="Left" Margin="-128,-12,0,-10" Width="30" Source="greenTick.png" RenderTransformOrigin="0.533,0.26"/>
            <Button x:Name="install_Nodejs_btn" Content="Install NodeJS 8.6" Click="installNodejs" HorizontalAlignment="Left" Margin="758,-1,-117,0" Width="200" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Height="35" VerticalAlignment="Top"/>
        </Grid>
        <Button x:Name="install_Bower_btn" Content="Install Bower" Click="installBower" HorizontalAlignment="Left" Margin="564,336,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Grid.Column="3" Grid.ColumnSpan="3"/>
        <Label x:Name="ember_version_label" Content="" HorizontalAlignment="Left" Margin="159,413,0,0" VerticalAlignment="Top" Width="533" Height="37" Foreground="#FFF8FAFF" FontSize="18" TextOptions.TextHintingMode="Animated" UseLayoutRounding="False" FontFamily="Sylfaen" Grid.ColumnSpan="4"/>
        <Button x:Name="install_Ember_btn" Content="Install Ember" Click="installEmber" HorizontalAlignment="Left" Margin="565,414,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Grid.Column="3" Grid.ColumnSpan="3"/>
        <Button x:Name="config_btn" Content="    Configurations       " Click="configWindow" Margin="70,497,0,0" VerticalAlignment="Top" Height="41" FontWeight="Bold" FontSize="18" Background="#FFFFAE00" Foreground="#FF1B1B1B" Grid.Column="3" HorizontalAlignment="Left" Width="182"/>
        <Image x:Name="java_install_sign" HorizontalAlignment="Left" Height="59" Margin="103,98,0,0" VerticalAlignment="Top" Width="30" Source="greenTick.png" RenderTransformOrigin="0.581,0.486"/>
        <Image x:Name="ember_install_sign" HorizontalAlignment="Left" Height="59" Margin="103,414,0,0" VerticalAlignment="Top" Width="30" Source="greenTick.png" RenderTransformOrigin="0.114,0.686"/>
        <Button x:Name="path_java_btn" Content="path to OpenJDK 1.8" Click="pathToJava" HorizontalAlignment="Left" Margin="316,97,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Grid.Column="3"  />
        <Button x:Name="path_bower_btn" Content="path to Bower" Click="pathToBower" HorizontalAlignment="Left" Margin="316,336,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Grid.Column="3"  />
        <Button x:Name="path_ember_btn" Content="path to Ember" Click="pathToEmber" HorizontalAlignment="Left" Margin="317,414,0,0" VerticalAlignment="Top" Width="200" Height="35" FontWeight="Bold" FontSize="18" Background="#FFF8FAFF" Foreground="#FF00081D" Grid.Column="3"  />
        <Label x:Name="process_info_label" Content="" Grid.Column="3" HorizontalAlignment="Left" Margin="335,526,0,0" VerticalAlignment="Top" Width="238" Height="40" Foreground="#FFBBA900" FontSize="18" FontFamily="Sylfaen"/>
        <Grid Grid.ColumnSpan="6" HorizontalAlignment="Left" Height="47" VerticalAlignment="Top" Width="1300" Background="#FF525252"/>
        <Button x:Name="close" Content="" Grid.Column="5" HorizontalAlignment="Left" Margin="106,3,0,0" VerticalAlignment="Top" Width="40" Height="40" RenderTransformOrigin="-0.08,0.643" Foreground="{x:Null}" BorderBrush="{x:Null}" Click="closeProgram">
            <Button.Background>
                <ImageBrush ImageSource="redCross.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="minimize" Content="" Grid.Column="5" HorizontalAlignment="Left" Margin="55,3,0,0" VerticalAlignment="Top" Width="40" Height="40" RenderTransformOrigin="-0.08,0.643" Foreground="{x:Null}" BorderBrush="{x:Null}" Click="minimizeProgram">
            <Button.Background>
                <ImageBrush ImageSource="min-01.png"/>
            </Button.Background>
        </Button>
        <Label Content="AJAN CONTROL PANEL" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="352" Height="58" FontSize="25" FontFamily="Sylfaen" Foreground="White" Grid.ColumnSpan="3"/>
        <Image x:Name="bower_install_sign" HorizontalAlignment="Left" Height="59" Margin="103,337,0,0" VerticalAlignment="Top" Width="30" Source="greenTick.png" RenderTransformOrigin="0.429,0.586"/>
        <Label x:Name="oneLinerLogLabel" Content="real time application status..." HorizontalAlignment="Left" Margin="173,781,0,0" VerticalAlignment="Top" Width="1000" Grid.ColumnSpan="6" Foreground="#FFFDFDFD" FontSize="15" MaxHeight="35" MaxWidth="1000"/>
        <Grid Grid.ColumnSpan="4" Margin="177,758,32,38">
            <ProgressBar x:Name="setupProgressBar" HorizontalAlignment="Left" Height="28" VerticalAlignment="Top" Width="876" BorderBrush="#FF06B025" Margin="0,1,0,0"/>
            <Label x:Name="ProgressBarPercent" Content="0%" HorizontalAlignment="Left" Margin="423,-1,0,0" FontSize="15" FontWeight="Bold" RenderTransformOrigin="0.562,0.517" Height="30" VerticalAlignment="Top"/>

        </Grid>
        <Label x:Name="currentTaskLabel" Content="current task" HorizontalAlignment="Left" Margin="45,726,0,0" VerticalAlignment="Top" Width="277" Foreground="#FFFDFDFD" FontSize="15" MaxHeight="35" MaxWidth="1000" Grid.Column="3" HorizontalContentAlignment="Center" FontWeight="Bold" FontFamily="Comic Sans MS"/>

    </Grid>
</Window>
